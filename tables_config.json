{
  "json_files": {
    "modules.json": {
      "table": "modules",
      "fields": ["TenantId", "Org", "ProjectId", "ProjectName", "ModuleId", "ModuleName", "ModuleUrl", "IsReadOnlyModule", "VersionType", "Version", "ParentModuleId", "Type"]
    },
    "workspaces.json": {
      "table": "workspaces",
      "fields": ["ModuleId", "Id", "Title", "Name"],
      "children": {
        "PageGroups": {
          "table": "page_groups", 
          "fields": ["ModuleId", "Id", "Title", "Name"],
          "parent_id": "WorkspaceId"
        },
        "Pages": {
          "table": "pages",
          "fields": ["ModuleId", "Id", "Title", "Name"],
          "parent_id": "PageGroupId",
          "children": {
            "Views": {
              "table": "views",
              "fields": ["ModuleId", "Id", "WorkspaceId", "PageId", "Title", "Name"],
              "parent_id": "PageId"
            }
          }
        }
      }
    },
    "action_buttons.json": [
      {
        "table": "action_buttons_ibpl_rules",
        "fields": ["ModuleId", "Id", "ActionButtonId", "Name", "Description", "Tooltip", "ActionButtonType", "IsGlobal" ],
        "children": {
          "ConfigJson.IBPLRules": {
            "table": "action_buttons_ibpl_rules",
            "fields": ["template"],
            "children": {
              "output": {
                "table": "action_buttons_ibpl_rules",
                "fields": ["name", "type"]
              },
              "input": {
                "table": "action_buttons_ibpl_rules",
                "fields": ["name", "type"]
              }
            }
          }
        }
      },
      {
        "table": "action_buttons_javascript_rules",
        "fields": ["ModuleId", "Id", "ActionButtonId", "Name", "Description", "Tooltip", "ActionButtonType", "IsGlobal"],
        "children": {
          "ConfigJson.JavascriptRules": {
            "table": "action_buttons_javascript_rules",
            "fields": ["modulename", "functionname"]
          }
        }
      },
      {
        "table": "action_buttons_fieldbindings",
        "fields": ["ModuleId", "Id", "ActionButtonId", "Name", "Description", "Tooltip", "ActionButtonType", "IsGlobal"],
        "children": {
          "ConfigJson.FieldBindings": {
            "table": "action_buttons_fieldbindings",
            "fields": ["fieldName", "title", "type", "selectionType", "clearSearch", "source", "defaultValue",
            "visible", "isEditable"],
            "children": {
              "validation": {
                "table": "action_buttons_fieldbindings",
                "fields": ["maxLength", "required"]
              }
            }
          }
        }
      }
    ],
    "rule_groups.json": [
      {
        "filters": {"RuleGroupType": ["Regular", "Spreading", "Block", "Cartesian", "EvaluateMember", "Graph", "Recurrence"]},
        "table": "active_rules",
        "fields": ["ModuleId", "RuleGroupLabelId", "ScopeLabelId","IsEnabled", "RuleGroupType"],
        "children": {
          "RuleGroupContent": {
            "table": "active_rules",
            "fields": ["RuleGroupText"]
          }
        }
      },
      {
        "filters": {"RuleGroupType": "ParameterizedProcedure"},
        "table": "procedures",
        "fields": ["ModuleId", "RuleGroupType", "RuleGroupName", "RuleGroupDescription", "RuleGroupLabelId","IsEnabled"],
        "children": {
          "RuleGroupContent": {
            "table": "procedures",
            "fields": ["RuleGroupText"]
          }
        }
      },
      {
        "filters": {"RuleGroupType": "NamedNodeFilterSet"},
        "table": "named_node",
        "fields": ["ModuleId", "RuleGroupType", "RuleGroupDescription", "RuleGroupLabelId","IsEnabled"],
        "children": {
          "RuleGroupContent": {
            "table": "named_node",
            "fields": ["NamedNodeName", "RuleGroupText"]
          }
        }
      },
      {
        "filters": {"RuleGroupType": "NamedSet"},
        "table": "named_set",
        "fields": ["ModuleId", "RuleGroupType", "RuleGroupName","RuleGroupDescription", "RuleGroupLabelId","IsEnabled"],
        "children": {
          "RuleGroupContent": {
            "table": "named_set",
            "fields": ["RuleGroupText"]
          }
        }
      }
    ],
    "rule_group_labels.json": {
      "table": "rule_groups_labels",
      "fields": ["ModuleId", "Id", "LabelName", "LabelDescription", "LabelType"]
    }
  }
}

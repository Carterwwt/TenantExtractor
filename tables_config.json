        "table": "pyspark_plugin_script",
        "fields": ["Id", "GlobalPluginId", "InstanceName", "Description", "InvocationType"],
        "children": {
          "ConfigJson": {
            "table": "pyspark_plugin_script",
            "fields": ["ScriptCode"]
          }
        }
      },
      {
        "filters": {"ClassName": ["o9.GraphCube.Plugins.Python.PySparkScript"]},
        "table": "pyspark_plugin_input_tables",
        "fields": ["Id", "GlobalPluginId", "InstanceName", "Description", "InvocationType"],
        "children": {
          "ConfigJson.InputTables": {
            "table": "pyspark_plugin_input_tables",
            "fields": ["VariableName", "Query", "Primary", "VariableType"]
          }
        }
      },
      {
        "filters": {"ClassName": ["o9.GraphCube.Plugins.Python.PySparkScript"]},
        "table": "pyspark_plugin_output_tables",
        "fields": ["Id", "GlobalPluginId", "InstanceName", "Description", "InvocationType"],
        "children": {
          "ConfigJson.OutputTables": {
            "table": "pyspark_plugin_output_tables",
            "fields": ["VariableName", "VariableType"]
          }
        }
      },
      {
        "filters": {"ClassName": ["o9.GraphCube.Plugins.RScript.Generalized.RScriptGeneralized"]},

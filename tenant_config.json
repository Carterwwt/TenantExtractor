{
  "json_files": {
    "workspaces.json": {
      "table": "workspaces",
      "fields": ["WorkspaceId", "Title", "Name"],
      "children": {
        "PageGroups": {
          "table": "page_groups", 
          "fields": ["Id", "Title", "Name"],
          "parent_id": "WorkspaceId"
        }
      }
    },
    "action_buttons.json": {
      "table": "action_buttons",
      "fields": ["Id", "ActionButtonId", "Name", "Description", "Tooltip", "ActionButtonType", "IsGlobal"],
      "children": {
        "IBPLRules": {
          "table": "ibpl_rules",
          "fields": ["template", "output", "input"]
        },
        "JavascriptRules": {
          "table": "javascript_rules", 
          "fields": ["modulename", "functionname"]
        },
        "FieldBindings": {
          "table": "field_bindings",
          "fields": ["fieldName", "title", "type", "selectionType", "clearSearch", "source", "defaultValue", "visible", "isEditable"]
        }
      }
    }
  }
}
